    "import numpy as np\n",
    "\n",
    "def accuracy_score(y_true, y_pred):\n",
    "    correct = 0\n",
    "    for true, pred in zip(y_true, y_pred):\n",
    "        if true==pred:\n",
    "            correct += 1\n",
    "    return correct/len(y_true)"
   ]
