    "import numpy as np\n",
    "\n",
    "def transform_basis(B: list[list[int]], C: list[list[int]]) -> list[list[float]]:\n",
    "\tP = np.linalg.inv(np.array(C)) @ np.array(B)\n",
    "\treturn P"
